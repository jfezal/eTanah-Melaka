        </txncode>
        <report></report>
    </stage>
    <stage id="Semakan" currentAction = "Semakan Kemasukan Maklumat" listener="etanah.view.hasil.validator.AnsuranValidation">
        <txncode id="BACT" daysToComplete = "2">
