<?xml version="1.0" encoding="UTF-8"?>
<workflow id="http://xmlns.oracle.com/Hasil/Project1/Humantask1">
    <stage id="Kemasukan" currentAction = "Kemasukan Maklumat" listener="etanah.view.hasil.validator.AnsuranValidation">
