package etanah.view.penguatkuasaan;

import etanah.model.Permohonan;
import etanah.model.PermohonanTuntutanKos;

import java.io.Serializable;
import java.math.BigDecimal;

/**
 *
 * @author latifah.iskak
 *
 */
public class BayaranAnsuranValue implements Serializable {

   Long idKos;

    String kodTransaksi;

    String kodUrusan;

    String namaUrusan;

    BigDecimal amaun;

    String senaraiHakmilik;

    Permohonan permohonan;

    PermohonanTuntutanKos tuntutKos;

    String idPermohonan;

    int kuantiti = 1;
    
    BigDecimal baki;
    
    int bilBulanAnsuran;

    public Long getIdKos() {
        return idKos;
    }

    public void setIdKos(Long idKos) {
        this.idKos = idKos;
    }

    public String getKodTransaksi() {
        return kodTransaksi;
    }

    public void setKodTransaksi(String kodTransaksi) {
        this.kodTransaksi = kodTransaksi;
    }

    public String getKodUrusan() {
        return kodUrusan;
    }

    public void setKodUrusan(String kodUrusan) {
        this.kodUrusan = kodUrusan;
    }

    public String getNamaUrusan() {
        return namaUrusan;
    }

    public void setNamaUrusan(String namaUrusan) {
        this.namaUrusan = namaUrusan;
    }

    public BigDecimal getAmaun() {
        return amaun;
    }

    public void setAmaun(BigDecimal amaun) {
        this.amaun = amaun;
    }

    public String getSenaraiHakmilik() {
        return senaraiHakmilik;
    }

    public void setSenaraiHakmilik(String senaraiHakmilik) {
        this.senaraiHakmilik = senaraiHakmilik;
    }

    public Permohonan getPermohonan() {
        return permohonan;
    }

    public void setPermohonan(Permohonan permohonan) {
        this.permohonan = permohonan;
    }

    public PermohonanTuntutanKos getTuntutKos() {
        return tuntutKos;
    }

    public void setTuntutKos(PermohonanTuntutanKos tuntutKos) {
        this.tuntutKos = tuntutKos;
    }

    public String getIdPermohonan() {
        return idPermohonan;
    }

    public void setIdPermohonan(String idPermohonan) {
        this.idPermohonan = idPermohonan;
    }

    public int getKuantiti() {
        return kuantiti;
    }

    public void setKuantiti(int kuantiti) {
        this.kuantiti = kuantiti;
    }

    public BigDecimal getBaki() {
        return baki;
    }

    public void setBaki(BigDecimal baki) {
        this.baki = baki;
    }

    public int getBilBulanAnsuran() {
        return bilBulanAnsuran;
    }

    public void setBilBulanAnsuran(int bilBulanAnsuran) {
        this.bilBulanAnsuran = bilBulanAnsuran;
    }
    
    
    
}
