package etanah.service.strata.cukaipetak;

import java.math.BigDecimal;
import java.math.BigInteger;

/**
 *
 * @author john
 */
public class KalkulatorCukaiPetak {
    
    public BigDecimal calc(double luas, double f) {
       System.out.print("asasasasas"+ Math.ceil(luas*f));
       BigDecimal luasd = new BigDecimal(String.valueOf(luas));
         BigDecimal rate = new BigDecimal(String.valueOf(f));
         double aaaa = luasd.multiply(rate).doubleValue();
        BigDecimal sum = new BigDecimal(Math.ceil(aaaa));
        
        return sum;
    }
}
