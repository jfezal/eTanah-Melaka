/*  NEW BUTTON*/
.round {
    cursor:pointer;
    cursor:hand;
    line-height:27px;
    height:27px;
    background:url(../images/button.gif) no-repeat right top;
    padding-right:30px;
    display:inline-block;
    text-decoration:none;
    border:0px;
}

.round ins {
    background:url(../images/button.gif) no-repeat left top;
    height:27px;
    line-height:27px;
    display:inline-block;
    padding-left:30px;
    text-decoration:none;
    color:black;
}

a.round:hover {background-position:right -155px;text-decoration:none}
a.round:hover ins {background-position:left -155px;text-decoration:none}
