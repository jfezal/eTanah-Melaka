<decorators defaultdir="/WEB-INF/decorators">
    <excludes>
        <pattern>/*maintab=true*</pattern>
        <pattern>/*tab=true*</pattern>
        <pattern>/*popup=true*</pattern>
        <pattern>/document/view*</pattern> <!-- for viewing document -->
        <pattern>/dokumen/simpan_imbasan*</pattern> <!-- saving scanned images -->
        <pattern>/util/test*</pattern> <!-- saving scanned images -->
        <pattern>/uam/daftar*</pattern> <!-- user registration -->
        <pattern>/utility/notifikasibayaran*</pattern> <!-- temp. use for iniatiate permohonan penguatkuasaan(strata) -->
        <pattern>/kiosk/tukarganti*</pattern>
        <pattern>/kiosk/status_permohonan*</pattern>
        <pattern>kiosk/main*</pattern>
        <!--<pattern>/WEB-INF/jsp/common/login.jsp</pattern>
        <pattern>/WEB-INF/jsp/login/login.jsp</pattern>-->
    </excludes>

    <decorator name="main" page="internal.jsp">
        <pattern>/WEB-INF/jsp/*</pattern>
        <pattern>/*</pattern>
    </decorator>
    <decorator name="mainTab" page="internal_tab.jsp">
        <pattern>/WEB-INF/jsp/tab/main_page.jsp</pattern>
    </decorator>
    <decorator name="internalmain" page="internalmain.jsp">
        <pattern>/WEB-INF/jsp/main/*</pattern>
    </decorator>
    <decorator name="internalLogin" page="internalLogin.jsp">
        <pattern>/WEB-INF/jsp/login/login.jsp</pattern>
    </decorator>
    <decorator name="internalRegister1" page="internalLogin.jsp">
        <pattern>/WEB-INF/jsp/uam/user_registration.jsp</pattern>
    </decorator>

    <decorator name="internalRegister3" page="internalLogin.jsp">
        <pattern>/WEB-INF/jsp/uam/lupaKataLaluan.jsp</pattern>
    </decorator>
    <decorator name="internalRegister3" page="internalLogin.jsp">
        <pattern>/WEB-INF/jsp/uam/forgotPassword.jsp</pattern>
    </decorator>
    <decorator name="internalRegister2" page="internalLogin.jsp">
        <pattern>/WEB-INF/jsp/uam/user_registration_1.jsp</pattern>
    </decorator>
    <decorator name="internalRegister" page="internalLogin.jsp">
        <pattern>/WEB-INF/jsp/login/firstTime_login.jsp</pattern>
    </decorator>
    <decorator name="internalRegister" page="internalLogin.jsp">
        <pattern>/WEB-INF/jsp/uam/semakan_permohonan.jsp</pattern>
    </decorator>
    <decorator name="internalKiosk" page="internalKiosk.jsp">
        <pattern>/WEB-INF/jsp/utiliti/pertanyaan_status_tkr_ganti.jsp</pattern>
    </decorator>
    <decorator name="internalKiosk" page="internalKiosk.jsp">
        <pattern>/WEB-INF/jsp/utiliti/paparan_status_tukar_ganti.jsp</pattern>
    </decorator>
    <decorator name="internalKiosk" page="internalKiosk.jsp">
        <pattern>/WEB-INF/jsp/utiliti/pertanyaan_status_permohonan.jsp</pattern>
    </decorator>
        <decorator name="internalKiosk" page="internalKiosk.jsp">
        <pattern>/WEB-INF/jsp/utiliti/paparan_status_permohonan.jsp</pattern>
    </decorator> 
    <decorator name="internalKiosk" page="internalKiosk.jsp">
        <pattern>/WEB-INF/jsp/utiliti/kiosk_main.jsp</pattern>
    </decorator>
    
    <decorator name="default" page="default.jsp">
    </decorator>  
</decorators>
