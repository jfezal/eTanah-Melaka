<%@ include file="/WEB-INF/jsp/include/page_header.jspf" %>
<%@page contentType="text/html" pageEncoding="windows-1252"%>
<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
    "http://www.w3.org/TR/html4/loose.dtd">

<s:form beanclass="etanah.view.pelupusan.PelupusanActionBean">
    <div class="subtitle">
        <p>KEMASUKAN BILANGAN MMK</p>
        <fieldset class="aras1">
            <legend>
                Carian
            </legend>
            <p>
                <label for="ID Berkaitan"> Bilangan Mesyuarat : </label>
                <s:text name=""/>
            </p>
            <p>
                <label for="ID Berkaitan"> Tarikh : </label>
                <s:text class="datepicker" name=""/>
            </p>
            <p>
                <label>&nbsp;</label>
                <s:reset name="" value="Isi Semula" class="btn"/>
                <s:submit name="" value="Cari" class="btn"/>
                <s:submit name="" value="Tambah" class="btn"/>
            </p>
        </fieldset>
    </div>

    <div class="subtitle">
        <fieldset class="aras1">
            <legend>
                Senarai Bilangan Mesyuarat Sedia Ada
            </legend>
            <p>
                <label>&nbsp;</label>
                <display:table name="" id="line" class="tablecloth">
                    <display:column title="No."/>
                    <display:column title="Bilangan Mesyuarat"/>
                    <display:column title="Tarikh Mesyuarat"/>
                </display:table>
            </p>
        </fieldset>
    </div>

    <div class="subtitle">
        <fieldset class="aras1">
            <legend>
                Senarai Permohonan Bagi Bilangan Mesyuarat
            </legend>
            <p>
                <label>&nbsp;</label>
                <display:table name="" id="line" class="tablecloth">
                    <display:column title="No."/>
                    <display:column title="Modul"/>
                    <display:column title="Kategori Permohonan"/>
                    <display:column title="ID Permohonan"/>
                    <display:column title="Nama Pemohon"/>
                    <display:column title="No. Kertas"/>    
                </display:table>
            </p>
            <p>
               
            </p>

        </fieldset>
        <p>
            <label>&nbsp;</label>
            <s:submit name="" value="Simpan" class="btn"/>
            <s:submit name="" value="Hantar" class="btn"/>
            <s:submit name="" value="Keluar" class="btn"/>
        </p>

    </div>


</s:form>
