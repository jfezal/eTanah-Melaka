            <plugin>
                <artifactId>maven-dependency-plugin</artifactId>
                <version>2.0</version>
                <executions>
                    <execution>
                        <id>copy</id>
                        <phase>prepare-package</phase>
                        <goals>
                            <goal>copy</goal>
                        </goals>
                        <configuration>
                            <artifactItems>
                                <artifactItem>
                                    <groupId>etanah</groupId>
                                    <artifactId>printApplet</artifactId>
                                    <version>2.0</version>
                                    <type>jar</type>
                                    <overWrite>true</overWrite>
                                    <destFileName>PrintApplet.jar</destFileName>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>etanah</groupId>
                                    <artifactId>scanApplet</artifactId>
                                    <version>1.0-SNAPSHOT</version>
                                    <type>jar</type>
                                    <overWrite>true</overWrite>
                                    <destFileName>ScanApplet.jar</destFileName>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>com.asprise</groupId>
                                    <artifactId>jtwain-artifact</artifactId>
                                    <version>1.0</version>
                                    <type>jar</type>
                                    <overWrite>true</overWrite>
                                    <destFileName>JTwain.jar</destFileName>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>com.mortennobel</groupId>
                                    <artifactId>java-image-scaling</artifactId>
                                    <version>0.8.5</version>
                                    <type>jar</type>
                                    <overWrite>true</overWrite>
                                    <destFileName>java-image-scaling.jar</destFileName>
                                </artifactItem>
                            </artifactItems>
                            <outputDirectory>${project.build.directory}/${artifactId}-${version}/</outputDirectory>
                        </configuration>
                    </execution>
                    <!--  <execution>
                        <id>copyScanApplet</id>
                        <phase>prepare-package</phase>
