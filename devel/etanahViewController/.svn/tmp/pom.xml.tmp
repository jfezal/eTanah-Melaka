        </profile>
        <profile>
            <id>pat-ns</id>
            <properties>
                <jarsigner.alias>etns-devcert</jarsigner.alias>
                <jarsigner.keystore>../etanahPom/etns-devcert.jks</jarsigner.keystore>
            </properties>
        </profile>
        <profile>
            <id>office-melaka</id>
            <properties>
                <jarsigner.skip>true</jarsigner.skip>
                <jarsigner.alias>etml-app</jarsigner.alias>
            </properties>
        </profile>
        <profile>
            <id>office-ns</id>
            <properties>
                <jarsigner.skip>true</jarsigner.skip>
                <jarsigner.alias>etns-app</jarsigner.alias>
            </properties>
        </profile>
        <profile>
            <id>office-jasin</id>
            <properties>
                <jarsigner.skip>true</jarsigner.skip>
                <jarsigner.alias>etml-app</jarsigner.alias>
            </properties>
        </profile>
		
    </profiles>

